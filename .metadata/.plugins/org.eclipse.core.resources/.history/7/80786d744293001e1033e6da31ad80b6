package com.Math;

public class Factors {

	public static void main(String[] args) {
		factorsOptimized(20);
	}
	static void factors(int n) {
		for(int i = 1 ; i <= n ; i++) {
			if(n % i == 0 && i != n) {
				System.out.print(i+", ");
			}else if(i == n){	
				System.out.print(i);
			}
		}
	}
	static void factorsOptimized(int n ) {
		for(int i = 1; i <= n/2 ; i++) {
			if(n % i == 0) {
				System.out.println(i+"\t"+(n/i));
			}
		}
	}
}
